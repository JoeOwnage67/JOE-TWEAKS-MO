<?xml version="1.0" encoding="utf-8"?>

<Patch>    
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Medieval Overhaul</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/FactionDef[@Name="DankPyon_NobleHouseFactionBase"]/caravanTraderKinds</xpath>
                    <value>
                        <li>Caravan_AlchemistTrader</li>
                        <li>Caravan_LibrarianTrader</li>
                        <li>Caravan_ProstheticsTrader</li>
                    </value>
                </li>    
             </operations>
        </match>
    </Operation>
    
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Medieval Overhaul: Arcane Archaeologists (Continued)</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/FactionDef[defName="Ogam_MagicalAcademy"]/caravanTraderKinds</xpath>
                    <value>
                        <li>Caravan_AlchemistTrader</li>
                        <li>Caravan_LibrarianTrader</li>
                        <li>Caravan_ProstheticsTrader</li>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>
