<?xml version="1.0" encoding="utf-8"?>

<Patch>
    <Operation Class="PatchOperationConditional">
	    <xpath>/Defs/ThingDef[defName="VFEM2_LowCastleWall"]/designationCategory</xpath>
		<match Class="PatchOperationReplace">
		    <xpath>/Defs/ThingDef[defName="VFEM2_LowCastleWall"]/designationCategory</xpath>
			<value>
			    <designationCategory>Structure</designationCategory>
		    </value>
		</match>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName="VFEM2_LowCastleWall"]</xpath>
			<value>
				<designationCategory>Structure</designationCategory>
			</value>
		</nomatch>         
    </Operation>

    <Operation Class="PatchOperationConditional">
	    <xpath>/Defs/ThingDef[defName="VFEM2_LowCastleWall"]/pathCost</xpath>
		<match Class="PatchOperationReplace">
		    <xpath>/Defs/ThingDef[defName="VFEM2_LowCastleWall"]/pathCost</xpath>
			<value>
			    <pathCost>500</pathCost>
		    </value>
		</match>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName="VFEM2_LowCastleWall"]</xpath>
			<value>
				<pathCost>500</pathCost>
			</value>
		</nomatch>         
    </Operation>
</Patch>

